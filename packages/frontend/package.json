{
	"name": "frontend",
	"private": true,
	"type": "module",
	"scripts": {
		"watch": "vite",
		"dev": "vite --config vite.config.local-dev.ts --debug hmr",
		"build": "vite build",
		"storybook-dev": "nodemon --verbose --watch src --ext \"mdx,ts,vue\" --ignore \"*.stories.ts\" --exec \"pnpm build-storybook-pre && pnpm exec storybook dev -p 6006 --ci\"",
		"build-storybook-pre": "tsc -p .storybook && node .storybook/preload-locale.js && node .storybook/preload-theme.js",
		"build-storybook": "pnpm build-storybook-pre && storybook build --webpack-stats-json storybook-static",
		"chromatic": "chromatic",
		"test": "vitest --run --globals",
		"test-and-coverage": "vitest --run --coverage --globals",
		"typecheck": "vue-tsc --noEmit",
		"eslint": "eslint --quiet \"src/**/*.{ts,vue}\"",
		"lint": "pnpm typecheck && pnpm eslint"
	},
